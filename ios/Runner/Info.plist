	<true/>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIMainStoryboardFile</key>
	<string>Main</string>
	<key>NSCameraUsageDescription</key>
    	<string>UChat needs to access your camera to change your profile picture</string>
    	<key>NSPhotoLibraryUsageDescription</key>
    	<string>UChat needs to access your photos to change your profile picture</string>
